<query id="insertStadium" action="insert">
    <tables>
        <table name="bfc_attendance_stadiums" />
    </tables>
    <columns>
        <column name="stadium_srl" var="stadium_srl" filter="number" notnull="notnull" />
        <column name="title" var="title" notnull="notnull" />
        <column name="lat" var="lat" notnull="notnull" />
        <column name="lon" var="lon" notnull="notnull" />
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
</query>